{
  "name": "@wppconnect/server",
  "version": "1.6.2",
  "description": "Projeto feito para autenticar a automomacao do WhatsappWeb com multi-clientes de forma dinamica. Backend feito em Nodejs(express, socketio), FrontEnd (ReactJS)",
  "main": "dist/index.js",
  "author": "kingaspx",
  "license": "Apache-2.0",
  "scripts": {
    "build": "rimraf dist && babel src -s -D -d dist",
    "changelog:last": "conventional-changelog -p angular -r 2",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git-cz",
    "dev": "nodemon -w src --exec \"babel-node src/server.js\"",
    "license:add": "license-check-and-add add",
    "license:check": "license-check-and-add check",
    "lint": "eslint --ext .js src",
    "prepare": "husky install",
    "release": "release-it",
    "start": "node ./dist/server.js",
    "swagger:copy-assets": "shx cp -R ./node_modules/swagger-ui-dist ./swagger-docs",
    "swagger:copy-json": "shx cp ./src/swagger.json ./swagger-docs",
    "swagger:fix-url": "sed -i \"s|https://petstore.swagger.io/v2/swagger.json|./swagger.json|g\" ./swagger-docs/index.html",
    "swagger:generate": "yarn swagger:copy-assets && yarn swagger:copy-json && yarn swagger:fix-url"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@wppconnect-team/wppconnect": "^1.17.0",
    "axios": "^0.27.0",
    "bcrypt": "^5.0.1",
    "buffer-to-stream": "1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-query-boolean": "^2.0.0",
    "form-data": "^4.0.0",
    "json-mapper-json": "^1.3.3",
    "merge-deep": "^3.0.3",
    "moment": "^2.29.4",
    "multer": "^1.4.4",
    "socket.io": "^4.5.2",
    "swagger-ui-express": "^4.5.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/node": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/cz-commitlint": "^17.1.2",
    "aws-sdk": "^2.1223.0",
    "commitizen": "^4.2.5",
    "conventional-changelog-cli": "^2.2.2",
    "crypto-js": "^4.1.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "license-check-and-add": "^4.0.5",
    "mongoose": "^6.6.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "redis": "^4.3.1",
    "release-it": "^15.4.2",
    "rimraf": "^3.0.2",
    "shx": "^0.3.4",
    "swagger-ui-dist": "^4.14.1",
    "webpack-cli": "^4.10.0"
  },
  "peerDependencies": {
    "aws-sdk": "^2.1223.0",
    "crypto-js": "^4.1.1",
    "mongoose": "^6.6.2",
    "redis": "^4.3.1"
  }
}
